	<div class='button_new_project'>
		<%= link_to "+", new_project_path, :class => 'btn1', :method=> :get %>
				</div>
<div class='projects_container'>
<% @projects.each do |project| %>
	<div class="row">
	  <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
	    <div class="thumbnail">
	      <img src="/assets/example1.jpg" />
	      <div class="caption">
	        <h3><%= project.name %></h3>
	        <p><%= project.description %></p>
	        <p><%= link_to 'EDIT', edit_project_path(project), :class => 'btn btn-default' %>
	        	<%= link_to 'MORE DETAILS', project_path(project), :class => 'btn btn-default' %>
	        	<%= link_to 'DELETE', project_path(project), :class => 'btn btn-default', method: :delete, data: { confirm: 'Are you sure?'} %></p>
	      </div>
	    </div>
	  </div>
	</div>
<% end %>
</div>


